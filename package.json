{
  "name": "gcstatus",
  "version": "1.0.0",
  "description": "Application to check game crack statuses",
  "main": "./src/main.tsx",
  "scripts": {
    "start": "concurrently \"npm:dev\" \"npm:storybook\"",
    "dev": "vite --open",
    "build": "vite build",
    "preview": "vite preview",
    "prepare": "husky",
    "semantic-release": "semantic-release",
    "format": "prettier --write ./src && echo 'Formatting complete! Include any changes on your commit.'",
    "lint": "eslint --fix ./src && echo 'Lint complete.'",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:ci": "jest --ci --json --coverage --updateSnapshot",
    "type-check": "tsc && echo 'Type check complete.'",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
  "keywords": [
    "crack",
    "games",
    "status"
  ],
  "author": "Felipe Oliveira",
  "license": "ISC",
  "dependencies": {
    "@emotion/react": "^11.13.3",
    "@emotion/styled": "^11.13.0",
    "@mui/lab": "^6.0.0-beta.9",
    "@mui/material": "^6.1.0",
    "@reduxjs/toolkit": "^2.2.7",
    "@vitejs/plugin-react": "^4.3.1",
    "date-fns": "^3.6.0",
    "nprogress": "^0.2.0",
    "react": "^18.3.1",
    "react-calendar": "^5.0.0",
    "react-cookie": "^7.2.1",
    "react-countdown": "^2.3.6",
    "react-dom": "^18.3.1",
    "react-easy-crop": "^5.0.8",
    "react-helmet-async": "^2.0.5",
    "react-hook-form": "^7.53.0",
    "react-hot-toast": "^2.4.1",
    "react-icons": "^5.3.0",
    "react-redux": "^9.1.2",
    "react-responsive-carousel": "^3.2.23",
    "react-router-dom": "^6.26.2",
    "swiper": "^11.1.12"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.25.4",
    "@babel/preset-react": "^7.24.7",
    "@babel/preset-typescript": "^7.24.7",
    "@chromatic-com/storybook": "^2.0.2",
    "@commitlint/cli": "^19.5.0",
    "@commitlint/config-conventional": "^19.5.0",
    "@eslint/compat": "^1.1.1",
    "@faker-js/faker": "^9.0.0",
    "@storybook/addon-essentials": "^8.3.0",
    "@storybook/addon-interactions": "^8.3.0",
    "@storybook/addon-links": "^8.3.0",
    "@storybook/addon-postcss": "^2.0.0",
    "@storybook/addon-themes": "^8.3.0",
    "@storybook/blocks": "^8.3.0",
    "@storybook/react": "^8.3.0",
    "@storybook/react-vite": "^8.3.0",
    "@storybook/test": "^8.3.0",
    "@swc/core": "^1.7.26",
    "@swc/jest": "^0.2.36",
    "@testing-library/jest-dom": "^6.5.0",
    "@testing-library/react": "^16.0.1",
    "@types/glob": "^8.1.0",
    "@types/jest-axe": "^3.5.9",
    "@types/minimatch": "^5.1.2",
    "@types/nprogress": "^0.2.3",
    "@types/react-dom": "^18.3.0",
    "@typescript-eslint/eslint-plugin": "^8.5.0",
    "@typescript-eslint/parser": "^8.5.0",
    "autoprefixer": "^10.4.20",
    "concurrently": "^9.0.1",
    "cypress": "^13.14.2",
    "eslint": "^9.10.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.30.0",
    "eslint-plugin-jsx-a11y": "^6.10.0",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-react": "^7.36.1",
    "eslint-plugin-react-hooks": "^4.6.2",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "eslint-plugin-storybook": "^0.8.0",
    "husky": "^9.1.6",
    "jest": "^29.7.0",
    "jest-axe": "^9.0.0",
    "jest-environment-jsdom": "^29.7.0",
    "msw": "^2.4.5",
    "netlify-cli": "^17.38.1",
    "postcss": "^8.4.47",
    "prettier": "^3.3.3",
    "semantic-release": "^24.1.1",
    "storybook": "^8.3.0",
    "tailwindcss": "^3.4.11",
    "typescript": "^5.6.2",
    "vite": "^5.4.14",
    "whatwg-fetch": "^3.6.20"
  },
  "release": {
    "branches": [
      "main"
    ],
    "plugins": [
      [
        "@semantic-release/commit-analyzer",
        {
          "releaseRules": [
            {
              "type": "docs",
              "release": false
            },
            {
              "type": "refactor",
              "release": "patch"
            },
            {
              "type": "chore",
              "release": "patch"
            },
            {
              "scope": "no-release",
              "release": false
            }
          ]
        }
      ],
      "@semantic-release/release-notes-generator",
      "@semantic-release/github"
    ]
  },
  "msw": {
    "workerDirectory": "public"
  },
  "overrides": {
    "minimatch": "5.1.2",
    "glob": "8.1.0",
    "goober": "2.0"
  }
}
